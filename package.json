{
  "name": "mundobl-catalogo",
  "version": "0.1.0",
  "private": true,
  "author": "famantini@live.com.ar",
  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev --hostname 0.0.0.0",
    "build": "NEXT_TELEMETRY_DISABLED=1 prisma generate && next build --webpack",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "local:db": "bash scripts/local-db.sh",
    "local:dev": "bash scripts/local-db.sh && npm run dev",
    "migrate:supabase": "bash scripts/migrate-supabase.sh"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "antd": "^6.3.0",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "pg": "^8.18.0",
    "prisma": "^7.4.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@ant-design/nextjs-registry": "^1.3.0",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "npm-check-updates": "^19.3.2",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
